    //Funcion para el boton
    function guardarFormularioFS() {
        var fecha = document.getElementById('fechaFS').value;
        var acueducto = document.getElementById('acueductoFS').value;
        var toma = document.getElementById('tomaFS').value;
        var registro = document.getElementById('registroMFS').value;
        var direccion = document.getElementById('direccionFS').value;
        var encargado = document.getElementById('encargadoFS').value;
        var telefono = document.getElementById('telefonoFS').value;
        var funcionario = document.getElementById('funcionarioFS').value;
        var latitud = document.getElementById('LatitudFS').value;
        var longitud = document.getElementById('LongitudFS').value;
        var img = document.getElementById('imgFS').value;
        var limpieza = document.getElementById('FSLimpieza').value;
        if (limpieza == "Otro") {
            var especifique = document.getElementById('otroDescripFS').value;
        }
        else {
            var especifique = "NA"
        }
        var notas = document.getElementById('notasFS').value;
        var p1 = $('input:radio[name=infra1FS]:checked').val();
        var p2 = $('input:radio[name=infra2FS]:checked').val();
        var p3 = $('input:radio[name=infra3FS]:checked').val();
        var p4 = $('input:radio[name=infra4FS]:checked').val();
        var p5 = $('input:radio[name=infra5FS]:checked').val();
        var p6 = $('input:radio[name=infra6FS]:checked').val();
        var p7 = $('input:radio[name=infra7FS]:checked').val();
        var p8 = $('input:radio[name=infra8FS]:checked').val();
        var p9 = $('input:radio[name=infra9FS]:checked').val();
        var p10 = $('input:radio[name=infra10FS]:checked').val();
        var IoU = ""
        if (idForm == "") { /*Insert o update??*/
            IoU = "I"
        }
        else {
            IoU = "U"
        }


        try {
            $.ajax({
                url: '/Formularios/guardarFormularioFS/',
                type: 'Post',
                datatype: 'application/json',
                data: {
                    FECHA: fecha,
                    ACUEDUCTO: acueducto,
                    TOMA: toma,
                    REGISTRO: registro,
                    DIRECCION: direccion,
                    ENCARGADO: encargado,
                    TELEFONO: telefono,
                    FUNCIONARIO: funcionario,
                    LATITUD: latitud,
                    LONGITUD: longitud,
                    IMG: img,
                    LIMPIEZA: limpieza,
                    ESPECIFIQUE: especifique,
                    NOTAS: notas,
                    P1: p1,
                    P2: p2,
                    P3: p3,
                    P4: p4,
                    P5: p5,
                    P6: p6,
                    P7: p7,
                    P8: p8,
                    P9: p9,
                    P10: p10,
                    IOU: IoU,
                    ID: idForm,

                },
                success: function (data) {
                    console.log(data);
                    $('#fuenteSup').modal('toggle');
                    $("#infoGenFS")[0].reset();
                    $("#preguntasFS")[0].reset();
                    idForm = "";
                    if (IoU == "I") {
                        swal("Éxito", "Formulario ingresado correctamente", "success");
                    }
                    else {
                        swal("Éxito", "Formulario actualizado correctamente", "success");
                    }
                },
                error: function () {
                    swal("Error", "Error al crear o actualizar el formulario", "error");
                }
            });
        } catch (err) {
            console.log(err.message)
        }

