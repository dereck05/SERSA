<!--CREAR O EDITAR FORMULARIOS (MODALS)----------------------------------------------------------------------->
<!-- Formulario Fuente Superficial (FS)-->
<div id="fuenteSup" class="modal fade" style="overflow-y: scroll;" role="dialog" tabindex="-1">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">×</button>
            </div>
            <div class="modal-body">
                <div id="modal1FS">
                    <p style="text-align:center">Ficha de campo 1</p>
                    <p style="text-align:center;font-weight: bold;">Fuente Superficial</p>
                    <p style="font-weight: bold;">I. Información General</p>
                    @*Informacion general*@
                    <form id="infoGenFS">
                        <div class="form-group">
                            <label for="fecha">Fecha *</label>
                            <input class="form-control" type="date" id="fechaFS">
                        </div>

                        <div class="form-group">
                            <label for="acueducto">Nombre del acueducto *</label>
                            <input type="text" class="form-control" id="acueductoFS">
                        </div>

                        <div class="form-group">
                            <label for="acueducto">Nombre toma *</label>
                            <input type="text" class="form-control" id="tomaFS">
                        </div>

                        <div class="form-group">
                            <label for="encargado">Número de registro en MINAE *</label>
                            <input type="text" class="form-control" id="registroMFS">
                        </div>

                        <div class="form-group">
                            <label for="encargado">Registro en Dirección de ARS *</label>
                            <input type="text" class="form-control" id="direccionFS">
                        </div>

                        <div class="form-group">
                            <label for="encargado">Encargado del acueducto *</label>
                            <input type="text" class="form-control" id="encargadoFS">
                        </div>

                        <div class="form-group">
                            <label for="telefonoFS">Teléfono *</label>
                            <input type="text" class="form-control" id="telefonoFS">
                        </div>

                        <div class="form-group">
                            <label for="funcionario">Nombre del funcionario *</label>
                            <input type="text" class="form-control" id="funcionarioFS">
                        </div>

                        <div class="form-group">
                            <label for="Ubicacion">Ubicación *</label><br>
                            <label class="radio-inline"><input type="radio" id="UbicacionGpsFS" name="radio1" oninput='on_location_changeFS()'> GPS </label>
                            <label class="radio-inline"><input type="radio" id="UbicacionManualFS" name="radio1" oninput='on_location_changeFS()' checked> Manual </label>
                            <small id="ubihelpFS" class="form-text text-muted">Si no se encuentra en el lugar del acueducto, ingréselo de forma manual. </small>
                            <small id="ubihelp2FS" class="form-text text-muted"> Si usa GPS, espere un momento para cargar las coordenadas.</small>

                        </div>

                        <div class="form-group">

                            <label for="Latitud">Latitud *</label>
                            <input type="text" class="form-control" id="LatitudFS">

                            <label for="Longitud">Longitud *</label>
                            <input type="text" class="form-control" id="LongitudFS">
                        </div>
                        <div class="form-group">
                            @*Imagen se toma como no obligatoria*@
                            <label for="Image">Imagen</label><br>
                            <span class="btn btn-default btn-file">
                                <input type="file" id="imgFS">
                            </span>
                        </div>


                        <!-- Aca termina los campos en comun-->

                        <div class="form-group">
                            <label for="FSLimpieza">Frecuencia de Limpieza *</label><br>
                            <select id="FSLimpieza" name="FSLimpieza">
                                <option value="Nunca">Nunca</option>
                                <option value="Mensual">Mensual</option>
                                <option value="Semanal">Semanal</option>
                                <option value="Diario">Diario</option>
                                <option value="Otro">Otro</option>
                            </select>
                        </div>

                        <div class="form-group">
                            <label for="otroDescripFS">Especifique</label>
                            <input type="text" class="form-control" id="otroDescripFS" disabled>
                        </div>

                        @*Se toma como no obligatorio*@
                        <div class="form-group">
                            <label for="notasFS">Notas adicionales</label><br>
                            <textarea class="form-control" id="notasFS" name="notasFS" rows="2"></textarea>
                        </div>

                    </form>

                    <div class="alert alert-danger" role="alert" id="alert1FS">
                        Por favor ingrese todos los campos obligatorios
                    </div>

                    @*BOTON SIGUIENTE*@
                    <button id="btnNextModal1FS" class="btn btn-primary btn-lg" style="background-color: #4f83cc;color:#ffffff;float:right;">Siguiente</button>

                </div>
                <div id="modal2FS" class="hideModal">
                    <p style="text-align:center">Ficha de campo 1</p>
                    <p style="text-align:center;font-weight: bold;">Fuente Superficial</p>
                    <p style="font-weight: bold;">II. Diagnóstico de Infraestructura</p>
                    @*Diagnostico infraestructura*@
                    <form id="preguntasFS">
                        <div class="form-group">
                            <label for="infra1FS">1. ¿Está la captación fuera de un área protegida o zona de conservación? </label><br>
                            <label class="radio-inline"><input type="radio" id="infra1FS_si" name="infra1FS" value="Si"> Si </label>
                            <label class="radio-inline"><input type="radio" id="infra1FS_no" name="infra1FS" value="No"> No </label>
                        </div>

                        <div class="form-group">
                            <label for="infra2FS">2. ¿Está la toma de agua desprovista de infraestructura que la proteja? </label><br>
                            <label class="radio-inline"><input type="radio" id="infra2FS_si" name="infra2FS" value="Si"> Si </label>
                            <label class="radio-inline"><input type="radio" id="infra2FS_no" name="infra2FS" value="No"> No </label>
                        </div>

                        <div class="form-group">
                            <label for="infra3FS">3. ¿Está el área alrededor de la toma sin cerca?  </label><br>
                            <label class="radio-inline"><input type="radio" id="infra3FS_si" name="infra3FS" value="Si"> Si </label>
                            <label class="radio-inline"><input type="radio" id="infra3FS_no" name="infra3FS" value="No"> No </label>
                        </div>

                        <div class="form-group">
                            <label for="infra4FS">4.  ¿Está la toma de agua ubicada dentro de alguna zona de actividad agrícola? (crítica) </label><br>
                            <label class="radio-inline"><input type="radio" id="infra4FS_si" name="infra4FS" value="Si"> Si </label>
                            <label class="radio-inline"><input type="radio" id="infra4FS_no" name="infra4FS" value="No"> No </label>
                        </div>

                        <div class="form-group">
                            <label for="infra5FS">5. ¿Existe alguna otra fuente de contaminación alrededor de la toma (letrinas, animales, viviendas, basura o industrias, etc.)? (Observar si aproximadamente a 200 metros a la redonda existen letrinas, animales, viviendas, basura) (crítica) </label><br>
                            <label class="radio-inline"><input type="radio" id="infra5FS_si" name="infra5FS" value="Si"> Si </label>
                            <label class="radio-inline"><input type="radio" id="infra5FS_no" name="infra5FS" value="No"> No </label>
                        </div>

                        <div class="form-group">
                            <label for="infra6FS">6. ¿Está la captación con acceso fácil de personas y animales?(crítica) </label><br>
                            <label class="radio-inline"><input type="radio" id="infra6FS_si" name="infra6FS" value="Si"> Si </label>
                            <label class="radio-inline"><input type="radio" id="infra6FS_no" name="infra6FS" value="No"> No </label>
                        </div>

                        <div class="form-group">
                            <label for="infra7FS">7. ¿Están las rejillas de la toma en malas condiciones (ausentes, quebradas y otros)? </label><br>
                            <label class="radio-inline"><input type="radio" id="infra7FS_si" name="infra7FS" value="Si"> Si </label>
                            <label class="radio-inline"><input type="radio" id="infra7FS_no" name="infra7FS" value="No"> No </label>
                        </div>

                        <div class="form-group">
                            <label for="infra8FS">8. ¿Existe presencia de plantas (raíces, hojas y otros) tapando las rejillas de la toma?  </label><br>
                            <label class="radio-inline"><input type="radio" id="infra8FS_si" name="infra8FS" value="Si"> Si </label>
                            <label class="radio-inline"><input type="radio" id="infra8FS_no" name="infra8FS" value="No"> No </label>
                        </div>

                        <div class="form-group">
                            <label for="infra9FS">9. ¿Existen condiciones de deforestación y erosión en los alrededores de la toma de agua? </label><br>
                            <label class="radio-inline"><input type="radio" id="infra9FS_si" name="infra9FS" value="Si"> Si </label>
                            <label class="radio-inline"><input type="radio" id="infra9FS_no" name="infra9FS" value="No"> No </label>
                        </div>

                        <div class="form-group">
                            <label for="infra10FS">10. ¿Está ausente el desarenador después de la toma de agua? </label><br>
                            <label class="radio-inline"><input type="radio" id="infra10FS_si" name="infra10FS" value="Si"> Si </label>
                            <label class="radio-inline"><input type="radio" id="infra10FS_no" name="infra10FS" value="No"> No </label>
                        </div>

                    </form>

                    <div class="alert alert-danger" role="alert" id="alert2FS">
                        Por favor conteste todas las preguntas
                    </div>

                    @*BOTON ATRAS*@
                    <button id="btnBackModal2FS" class="btn btn-primary btn-lg" style="background-color: #4f83cc;color:#ffffff;">Atrás</button>
                    @*BOTON SIGUIENTE*@
                    <button id="btnNextModal2FS" class="btn btn-primary btn-lg" style="background-color: #4f83cc;color:#ffffff;float:right;">Siguiente</button>

                </div>
                <div id="modal3FS" class="hideModal">
                    <p style="text-align:center">Ficha de campo 1</p>
                    <p style="text-align:center;font-weight: bold;">Fuente Superficial</p>
                    <p style="font-weight: bold;">III. Filtro de campos</p>
                    <p>* Por favor seleccione los campos que desea incluir en el informe final</p>
                    <div class="form-check">
                        <input type="checkbox" class="form-check-input" id="filter_allFS">
                        <label class="form-check-label"> Deseo incluir toda la información registrada</label>
                    </div>
                    <div class="form-check">
                        <input type="checkbox" class="form-check-input" id="filter_fechaFS">
                        <label class="form-check-label"> Fecha</label>
                    </div>
                    <div class="form-check">
                        <input type="checkbox" class="form-check-input" id="filter_acueductoFS">
                        <label class="form-check-label"> Nombre del acueducto</label>
                    </div>
                    <div class="form-check">
                        <input type="checkbox" class="form-check-input" id="filter_encargadoFS">
                        <label class="form-check-label"> Nombre del encargado</label>
                    </div>
                    <div class="form-check">
                        <input type="checkbox" class="form-check-input" id="filter_telefonoFS">
                        <label class="form-check-label"> Teléfono</label>
                    </div>
                    <div class="form-check">
                        <input type="checkbox" class="form-check-input" id="filter_funcionarioFS">
                        <label class="form-check-label"> Nombre del funcionario</label>
                    </div>
                    <br />
                    <button id="btnBackModal3FS" class="btn btn-primary btn-lg" style="background-color: #4f83cc;color:#ffffff;">Atrás</button>
                    <button id="btnSaveFS" class="btn btn-primary btn-lg" style="background-color: #4f83cc;color:#ffffff;float:right;">Guardar</button>
                </div>
            </div>


        </div>
    </div>
</div>


<!-- Formulario Fuente Naciente (FN)-->
<div id="fuenteNac" class="modal fade" style="overflow-y: scroll;" role="dialog" tabindex="-1">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">×</button>
            </div>
            <div class="modal-body">
                <div id="modal1FN">
                    <p style="text-align:center">Ficha de campo 2</p>
                    <p style="text-align:center;font-weight: bold;">Fuente Naciente</p>
                    <p style="font-weight: bold;">I. Información General</p>
