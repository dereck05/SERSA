    //Si la opcion es Otro, se habilita el campo de Especifique
    function habilitarDescripcionFS() {
        var valor = document.getElementById('FSLimpieza').value;
        if (valor == "Otro") {
            document.getElementById("otroDescripFS").disabled = false;
        } else {
            document.getElementById("otroDescripFS").disabled = true;
            document.getElementById("otroDescripFS").value = "";
        }
    }
    //Funciones ubicacion (GPS) para fuente superficial
    function on_location_changeFS() {
        if (document.getElementById('UbicacionGpsFS').checked) {
            showPositionFS()
        } else if (document.getElementById('UbicacionManualFS').checked) {

            document.getElementById('LatitudFS').value = ''
            document.getElementById('LongitudFS').value = ''
        }
    }
    function showPositionFS() {
        if (navigator.geolocation) {
            navigator.geolocation.getCurrentPosition(function (position) {
                document.getElementById('LatitudFS').value = position.coords.latitude
                document.getElementById('LongitudFS').value = position.coords.longitude

            });
        } else {
            document.getElementById('LatitudFS').value = "GPS no disponible, por favor cambie a modo Manual"
            document.getElementById('LongitudFS').value = "GPS no disponible, por favor cambie a modo Manual"
        }
    }
