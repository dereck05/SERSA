        var p9 = $('input:radio[name=infra9PP]:checked').val();
        var p10 = $('input:radio[name=infra10PP]:checked').val();

        var IoU = ""
        if (idForm == "") {
            IoU = "I"
        }
        else {
            IoU = "U"
        }

        try {
            $.ajax({
                url: '/Formularios/guardarFormularioPP/',
                type: 'Post',
                datatype: 'application/json',
                data: {
                    FECHA: fecha,
                    ACUEDUCTO: acueducto,
